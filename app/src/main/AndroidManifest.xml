<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.zheng.demotest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".circleball.CircleActivity"/>
        <activity android:name=".myviewpage.MyViewPageActivity"/>
        <activity android:name=".quickquery.QuickQueryActivity"/>
        <activity android:name=".slidelayout.SlideActivity"/>
        <activity android:name=".wave.WaveActivity"/>
        <activity android:name=".flowlayout.FlowLayoutActivity"/>
        <activity android:name=".custombutton.CustomButtonActivity"/>
        <activity android:name=".handler.HandlerActivity"/>
        <activity android:name=".fragment.FragmentTestActivity"/>
        <activity android:name=".db.DbActivity"/>
        <activity android:name=".fragment.FragmentListActivity"/>
        <activity android:name=".fragment.FragmentContentActivity"/>
        <activity android:name=".okhttp.OkhttpActivity"/>
        <activity android:name=".serviceactivity.ServiceChangeActivityActivity"/>
        <service android:name=".serviceactivity.MyService"/>

        <activity android:name=".animation.AnimationActivity"/>
        <activity android:name=".animation.TweenActivity"/>
        <activity android:name=".animation.PropertyActivity"/>
        <activity android:name=".socket.SocketActivity"/>
        <service android:name=".socket.SocketService" android:process=":socket"/>




    </application>

</manifest>